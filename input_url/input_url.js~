var url;

function openUrl(url) {
    "use strict";
    /*jslint browser: true*/
    window.open(url, "mywindow", "width=400px,height=450px");
}


function urlInput() {
    "use strict";
    /*global prompt: true*/
    /*global alert: true*/
    var i, flag = 0;
    url = prompt("enter url");
    for (i = 0; i <  url.length; i += 1) {
        if (url[i] !== " ") {
            flag = 1;
        }
    }
    if (!flag) {
        alert("empty url");
        urlInput();
    }
    openUrl(url);
}


function initialize() {
    "use strict";
    urlInput();
    openUrl();
}
