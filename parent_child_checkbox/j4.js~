function colors(colors)
{
//alert("colors");
if(colors.checked)
{
//alert("red");
red = document.createElement("input");
red.setAttribute("type", "checkbox");
//red.id="red";
red.setAttribute("value","red");
red.checked=true;
//red.left=100px;
red.appendChild(document.createTextNode("red"));
colors.appendChild(red);
alert(red.checked);
}
}

